{
  "private": true,
  "name": "binpack",
  "version": "1.0.0",
  "description": "A template for kick starting a Cloudflare Workers project",
  "main": "dist/index.js",
  "engines": {
    "node": ">=13.2.0"
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "cfworker run src/index.ts --watch",
    "build": "cfworker build src/index.ts --out-file dist/index.js",
    "deploy": "cfworker deploy-dev src/index.ts",
    "format": "prettier --write '**/*.{js,css,json,md}'"
  },
  "author": "Jeason <jitong.zjt@alibaba-inc.com>",
  "license": "MIT",
  "devDependencies": {
    "@cfworker/dev": "^1.6.0",
    "@types/gunzip-maybe": "^1.4.0",
    "@types/tar-stream": "^2.1.0",
    "prettier": "^1.18.2",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "@cfworker/sentry": "^1.6.0",
    "@cfworker/web": "^1.6.0",
    "@cfworker/worker-types": "^1.6.0",
    "@cloudflare/workers-types": "^2.0.0",
    "gunzip-maybe": "^1.4.2",
    "js-untar": "^2.0.0",
    "path-browserify": "^1.0.1",
    "rollup": "^2.30.0",
    "tar-stream": "^2.1.4"
  }
}
